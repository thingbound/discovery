{
  "name": "tinkerhub-discovery",
  "version": "0.6.0",
  "license": "MIT",
  "description": "Standardized API for service discovery and publishing",
  "repository": "tinkerhub/tinkerhub-discovery",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "ci": "npm run coverage && npm run lint",
    "coverage": "jest --coverage",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "tsc --module commonjs --target es5 --outDir dist/cjs && tsc --module es6 --target es6 --outDir dist/esm",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "discovery",
    "service discovery"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "atvik": "^0.3.1",
    "debug": "^4.1.1",
    "fast-deep-equal": "^2.0.1",
    "ip-regex": "^4.1.0",
    "timer-wheel": "^0.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/node": "^8.0.0",
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
